{
  "name": "lumen-cms-core",
  "version": "0.54.3",
  "description": "Core website components for the LumenCMS",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src",
    "client"
  ],
  "module": "dist/lumen-cms-core.esm.js",
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": false,
    "singleQuote": true,
    "trailingComma": "none"
  },
  "scripts": {
    "build-ts": "rimraf dist && tsdx build -i src/index.tsx",
    "build-core": "rimraf dist && rimraf base/dist && tsdx build -i src/base.tsx --name lumen-cms-base && mv dist base/dist",
    "watch": "tsdx watch",
    "gen-ts-schema": "node generate-ts-schema.js",
    "fetch-storyblok-components": "storyblok pull-components --space=80001",
    "update-storyblok": "npm run fetch-storyblok-components && npm run gen-ts-schema && npm run codegen",
    "codegen": "graphql-codegen --config codegen.yml",
    "storybook": "start-storybook -p 6006 -c .storybook",
    "deploy-storybook": "rimraf public_storybook && build-storybook -c .storybook -o public_storybook && vercel --prod",
    "prepublishOnly": "npm run build-ts",
    "release": "np",
    "lint": "eslint './src/**/*.{ts,tsx,js}'",
    "lint:fix": "npm run lint -- --fix"
  },
  "author": "Dominic Garms",
  "license": "MIT",
  "engines": {
    "node": "12.x"
  },
  "dependencies": {
    "@charlietango/use-client-hydrated": "1.8.2",
    "@material-ui/core": "4.11.0",
    "@material-ui/lab": "4.0.0-alpha.56",
    "@react-hook/window-size": "3.0.7",
    "clsx": "1.1.1",
    "element-closest": "3.0.2",
    "intersection-observer": "0.11.0",
    "js-cookie": "2.2.1",
    "marked": "1.1.1",
    "mdi-material-ui": "6.18.0",
    "next-seo": "4.7.3",
    "nprogress": "0.2.0",
    "react-hooks-global-state": "1.0.1",
    "react-inlinesvg": "2.0.1",
    "react-intersection-observer": "8.27.1",
    "react-player": "2.6.1",
    "react-scroll-parallax": "2.3.4",
    "react-swipeable-views": "0.13.9",
    "react-swipeable-views-utils": "0.13.9",
    "storyblok-js-client": "2.5.2",
    "storyblok-react": "0.1.1",
    "swr": "0.3.1",
    "use-debounce": "3.4.3",
    "uuid": "^8.3.0"
  },
  "peerDependencies": {
    "next": "^9.5.2",
    "react-dom": "^16.13.1",
    "react": "^16.13.1"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^1.17.8",
    "@graphql-codegen/typescript": "^1.17.9",
    "@graphql-codegen/typescript-document-nodes": "^1.17.8",
    "@graphql-codegen/typescript-graphql-files-modules": "^1.17.8",
    "@graphql-codegen/typescript-operations": "^1.17.8",
    "@lumen/storyblok-material-icons": "git+https://github.com/dohomi/storyblok-fieldtype-material-icons-selector.git",
    "@storybook/addon-essentials": "^6.0.20",
    "@storybook/addon-knobs": "^6.0.20",
    "@storybook/react": "^6.0.20",
    "@types/debug": "^4.1.5",
    "@types/jest": "^26.0.9",
    "@types/js-cookie": "2.2.6",
    "@types/marked": "^1.1.0",
    "@types/match-sorter": "4.0.0",
    "@types/node": "^14.6.1",
    "@types/node-fetch": "2.5.7",
    "@types/nprogress": "0.2.0",
    "@types/react": "^16.9.48",
    "@types/react-swipeable-views": "0.13.0",
    "@types/uuid": "^8.3.0",
    "core-js": "^3.0.1",
    "dotenv": "^8.2.0",
    "eslint-config-airbnb-typescript-prettier": "3.1.0",
    "graphql": "^15.3.0",
    "jest": "^26.4.2",
    "json-schema-to-typescript": "^9.1.1",
    "lorem-ipsum": "^2.0.3",
    "next": "9.5.2",
    "np": "^6.4.0",
    "playwright": "1.3.0",
    "prettier": "^2.1.1",
    "qawolf": "^1.3.6",
    "ts-jest": "^26.3.0",
    "tsdx": "^0.13.3",
    "tslib": "^2.0.1",
    "typescript": "4.0.2"
  }
}
