{
  "name": "lumen-cms-core",
  "version": "0.60.6",
  "description": "Core website components for the LumenCMS",
  "repository": "https://github.com/lumen-cms/lumen-cms-core",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src",
    "client"
  ],
  "module": "dist/lumen-cms-core.esm.js",
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": false,
    "singleQuote": true,
    "trailingComma": "none"
  },
  "scripts": {
    "dev": "next",
    "build": "next build",
    "start": "next start",
    "analyze": "cross-env ANALYZE=true npm run build",
    "build-ts": "rimraf dist && tsdx build -i src/index.tsx --tsconfig ./tsconfig.build.json",
    "build-core": "rimraf dist && rimraf base/dist && tsdx build -i src/base.tsx --name lumen-cms-base --tsconfig ./tsconfig.build.json && mv dist base/dist",
    "watch": "tsdx watch",
    "gen-ts-schema": "node generate-ts-schema.js",
    "fetch-storyblok-components": "storyblok pull-components --space=82895",
    "update-storyblok": "npm run fetch-storyblok-components && npm run gen-ts-schema && npm run codegen",
    "codegen": "graphql-codegen --config codegen.yml",
    "storybook": "start-storybook -p 6006 -c .storybook",
    "build-story": "build-storybook -c .storybook -o public_storybook",
    "vercel-build": "build-storybook -c .storybook -o public",
    "deploy-storybook": "rimraf public_storybook && npm run build-story && vercel --prod",
    "prepublishOnly": "npm run build-ts",
    "release": "np",
    "lint": "eslint './src/**/*.{ts,tsx,js}'",
    "lint:fix": "npm run lint -- --fix"
  },
  "author": "Dominic Garms",
  "license": "MIT",
  "engines": {
    "node": "12.x"
  },
  "dependencies": {
    "@charlietango/use-script": "2.1.2",
    "@date-io/date-fns": "1.3.13",
    "@material-ui/core": "4.11.0",
    "@material-ui/icons": "4.9.1",
    "@material-ui/lab": "4.0.0-alpha.56",
    "@material-ui/pickers": "3.2.10",
    "@react-hook/window-size": "3.0.7",
    "@types/facebook-pixel": "0.0.20",
    "@types/gtag.js": "0.0.3",
    "@types/js-cookie": "2.2.6",
    "@types/nprogress": "0.2.0",
    "@types/react-swipeable-views": "0.13.0",
    "clsx": "1.1.1",
    "date-fns": "2.7.0",
    "deepdash": "^5.3.0",
    "element-closest": "3.0.2",
    "intersection-observer": "0.11.0",
    "js-cookie": "2.2.1",
    "lumen-cms-utils": "^1.0.0",
    "marked": "1.1.1",
    "mdi-material-ui": "6.19.0",
    "next-google-fonts": "^1.2.1",
    "next-seo": "4.10.0",
    "nprogress": "0.2.0",
    "react-form-hook-mui": "0.9.0",
    "react-hook-form": "6.7.2",
    "react-hooks-global-state": "1.0.1",
    "react-inlinesvg": "2.1.1",
    "react-intersection-observer": "8.29.0",
    "react-parallax": "3.0.3",
    "react-player": "2.6.2",
    "react-scroll-parallax": "2.3.4",
    "react-swipeable-views": "0.13.9",
    "react-swipeable-views-utils": "0.13.9",
    "storyblok-js-client": "3.1.0",
    "storyblok-react": "0.1.1",
    "swr": "0.3.5",
    "use-count-up": "2.2.5",
    "use-debounce": "5.0.1",
    "uuid": "^8.3.0"
  },
  "peerDependencies": {
    "next": "^9.5.3",
    "react-dom": "^16.13.1",
    "react": "^16.13.1"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^1.17.10",
    "@graphql-codegen/typescript": "^1.17.10",
    "@graphql-codegen/typescript-document-nodes": "^1.17.8",
    "@graphql-codegen/typescript-graphql-files-modules": "^1.18.0",
    "@graphql-codegen/typescript-operations": "^1.17.8",
    "@lumen/storyblok-material-icons": "git+https://github.com/dohomi/storyblok-fieldtype-material-icons-selector.git",
    "@next/bundle-analyzer": "^9.5.3",
    "@storybook/addon-essentials": "^6.0.22",
    "@storybook/addon-knobs": "^6.0.22",
    "@storybook/react": "^6.0.22",
    "@types/debug": "^4.1.5",
    "@types/jest": "^26.0.14",
    "@types/marked": "^1.1.0",
    "@types/match-sorter": "4.0.0",
    "@types/node": "^14.11.2",
    "@types/node-fetch": "2.5.7",
    "@types/react": "^16.9.49",
    "@types/uuid": "^8.3.0",
    "core-js": "^3.0.1",
    "cross-env": "^7.0.2",
    "dotenv": "^8.2.0",
    "eslint-config-airbnb-typescript-prettier": "3.1.0",
    "graphql": "^15.3.0",
    "jest": "^26.4.2",
    "json-schema-to-typescript": "^9.1.1",
    "lorem-ipsum": "^2.0.3",
    "lumen-cms-nextjs": "^1.8.0",
    "next": "9.5.3",
    "np": "^6.5.0",
    "playwright": "1.3.0",
    "prettier": "^2.1.2",
    "qawolf": "^1.4.0",
    "storyblok-generate-ts": "^0.6.0",
    "ts-jest": "^26.4.1",
    "tsdx": "^0.14.0",
    "tslib": "^2.0.1",
    "typescript": "4.0.3"
  }
}
